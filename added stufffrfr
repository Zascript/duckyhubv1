                ["ACIDUM-RIFLE"] = true,
                ["VENOM-BOW"] = true,
                ["SOUL-GUITAR"] = true,
            },
            ["FRUITPURCHASE"] = true,
            ["PRIORITYFRUIT"] = {
                [1] = "Dragon-Dragon",
                [2] = "Flame-Flame",
                [3] = "Rumble-Rumble",
                [4] = "Human-Human: Buddha",
                [5] = "Dark-Dark",
