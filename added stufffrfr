    Callback = function()
        local success, response = pcall(function()
            return game:HttpGet("https://raw.githubusercontent.com/Zascript/anti-afk/main/anti%20afk%20ahhh")
        end)
        if success then
            loadstring(response)()
        else
            warn("Failed to load Anti AFK script.")
        end
    end
})

