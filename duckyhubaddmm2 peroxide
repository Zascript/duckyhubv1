Tabs.MM2:AddButton({
    Title = "mobile + pc script op",
    Callback = function()
        -- Best configs that don't kick you
        _G.AutofarmSettings = {
            AntiAfk = true, 
            DelayFarm = 2.15,
            ResetWhenFullBag = true,
            CoinType = "BeachBall",
            Disable3DRendering = false,
            ImproveFPS = false,
            StartAutofarm = false
        }
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NoCapital2/MM2Autofarm/main/script"))()  
